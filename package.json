{
  "name": "publisher-subscriber",
  "version": "0.3.0",
  "description": "Publisher Subscriber  Mechanism ",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "",
  "dependencies": {
    "tslib": "^2.4.1"
  },
  "devDependencies": {
    "@types/jest": "^29.2.3",
    "@types/redis": "^2.8.27",
    "@types/redis-mock": "^0.17.1",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "4.9",
    "@open-wc/building-rollup": "^1.9.4",
    "@web/dev-server": "0.0.29",
    "concurrently": "^5.3.0",
    "deepmerge": "^4.2.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.34.2"
  },
  "scripts": {
    "build": "rm -rf ./dist & tsc -b tsconfig.build.json",
    "test": "jest",
    "prepublish": "npm run build",
    "dev": "npm run build && ts-node-dev --respawn -- dist/index.js",
    "tsc:watch": "tsc --watch",
    "start:single": "concurrently --kill-others --names tsc,web-dev-server \"npm run tsc:watch\" \"web-dev-server --app-index index.html --node-resolve --open --watch\"",
    "start": "concurrently --kill-others --names tsc,web-dev-server \"npm run tsc:watch\" \"web-dev-server --config web-dev-server.config.js\""
  },
  "keywords": [
    "pubsub",
    "typescript",
    "publisher subscriber",
    "observer",
    "design patterns"
  ],
  "author": "Valodya Papikyan",
  "license": "MIT"
}
